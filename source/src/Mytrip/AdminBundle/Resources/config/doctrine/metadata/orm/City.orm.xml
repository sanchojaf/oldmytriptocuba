<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="City" table="city">
    <id name="cityId" type="integer" column="city_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="city" type="string" column="city" length="255" nullable="false"/>
    <field name="stateId" type="integer" column="state_id" nullable="false"/>
    <field name="countryId" type="integer" column="country_id" nullable="false"/>
    <field name="createdDate" type="datetime" column="created_date" nullable="false"/>
  </entity>
</doctrine-mapping>
