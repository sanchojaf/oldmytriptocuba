<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="User" table="user">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="50" nullable="false"/>
    <field name="lastname" type="string" column="lastname" length="50" nullable="false"/>
    <field name="username" type="string" column="username" length="150" nullable="false"/>
    <field name="password" type="string" column="password" length="150" nullable="false"/>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="phone" type="string" column="phone" length="50" nullable="false"/>
    <field name="mobile" type="string" column="mobile" length="50" nullable="false"/>
    <field name="address" type="string" column="address" length="150" nullable="false"/>
    <field name="address2" type="string" column="address2" length="150" nullable="false"/>
    <field name="city" type="string" column="city" length="50" nullable="false"/>
    <field name="province" type="string" column="province" length="50" nullable="false"/>
    <field name="country" type="string" column="country" length="50" nullable="false"/>
    <field name="lan" type="string" column="lan" length="2" nullable="false"/>
    <field name="status" type="string" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
